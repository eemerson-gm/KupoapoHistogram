#                     Dependencies
# =======================================================
./obj/DP-2.o : ./src/DP-2.c ../inc/Header.h
	cc -c ./src/DP-2.c -o ./obj/DP-2.o -lpthread

#
# =======================================================
# Other targets
# =======================================================
clean:
	rm -f ./bin/*
	rm -f ./obj/*
	rm -f ./src/DP-2.c~
